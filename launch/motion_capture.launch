<?xml version="1.0"?>
<launch>
    <arg name="relay" default="false" doc="Act as a motion capture relay"/>
    <node pkg="panda_torque_mpc" type="motion_capture_relay.py" name="motion_capture_relay_node"
        required="true" output="screen">
        <param name="relay" value="$(arg relay)"/>
        <rosparam command="load" file="$(find panda_torque_mpc)/config/motion_capture_params.yaml" />
    </node>
</launch>